<!-- Question 5 -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Question 5</title>
</head>
<body>
    <script>
        
      var msg = 'Enter the total sales:';
    
      var a = prompt(msg);
      if(a<=5000 && a>=0)
        
            document.write("Commision earned is " +(a*.02));
      else if(a<=10000 && a>=5001)
          
            document.write("Commision earned is " +(a*.05)); 
      else if(a<=20000 && a>=10001)
         
            document.write("Commision earned is " +(a*.07));
      else if(a>=10001)
          
            document.write("Commision earned is "+(a*.10));

      </script>
</body>
</html>


