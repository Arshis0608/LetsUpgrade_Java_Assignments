<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=, initial-scale=1.0">
    <title>Question 6</title>
</head>
<body>
    <script>
    let num;

do {
  num = prompt("Enter a number greater than 100?", 0);
} while (num <= 100 && num);

if(num>100)
        
        document.write("Thank You");

  else if(num<=100)
      
        document.write("Please Enter A value"); 
 

</script>
</body>
</html>
        

<!-- Question 7 -->

<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Question 7</title>
</head>
<body>
<script>
    let n = prompt("Write the n Value");
    let result = 0;
    
    outer:
    for (let i = 2; i < n; i++) {
        for (let j = 2; j < i; j++) {
            if (i%j == 0) {
                continue outer
            }
        }
        document.write(i);
        console.log(i);
    
    }   
</script> 
</body>
</html>

