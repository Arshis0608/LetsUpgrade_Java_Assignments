    <script>
    let num;

do {
  num = prompt("Enter a number greater than 100?", 0);
} while (num <= 100 && num);

if(num>100)
        
        document.write("Thank You");

  else if(num<=100)
      
        document.write("Please Enter A value"); 
 

</script>
</body>
