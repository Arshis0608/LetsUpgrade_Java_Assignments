    <title>Question 7</title>
</head>
<body>
<script>
    let n = prompt("Write the n Value");
    let result = 0;
    
    outer:
    for (let i = 2; i < n; i++) {
        for (let j = 2; j < i; j++) {
            if (i%j == 0) {
                continue outer
            }
        }
        document.write(i);
        console.log(i);
    
    }   
</script> 
</body>
</html>

